<script>
	//window.onload = function() {
	(function() {
		// https://stackoverflow.com/questions/12539361/how-to-reverse-the-ordering-of-list-items-in-an-unordered-list
		window.reverse = function() {
			var list = document.querySelector("#feed");
			var i = list.childNodes.length;
			while (i--)
			  list.appendChild(list.childNodes[i]);
			  console.log(i);
		}
		
		document.querySelectorAll('ul#security a').forEach(function(el, i) {
			el.onclick = function(e) {
				if (el.id == 'locked') {
					elems = '.locked';
					toggle_class = 'exclude_locked';
				} else if (el.id == 'unlocked') {
					elems = ':not(.locked)';
					toggle_class = 'exclude_not_locked';
				}
				if (!el.classList.contains('current')) {
					el.classList.add('current');
					/*
					document.querySelectorAll('ul#feed > li' + elems).forEach(function(el, i) {
						el.style.display  = 'block';
					});
					*/
				} else {
					el.classList.remove('current');
					/*
					document.querySelectorAll('ul#feed > li' + elems).forEach(function(el, i) {
						el.style.display  = 'none';
					});
					*/
				}
				document.querySelector('ul#feed').classList.toggle(toggle_class);
				document.getElementById('current_show').innerHTML = document.querySelectorAll('ul#feed > li').length - (document.querySelectorAll('ul#feed.exclude_not_locked > li:not(.locked)').length + document.querySelectorAll('ul#feed.exclude_locked > li.locked').length);
				e.preventDefault();
			}
		});
		
		document.querySelectorAll('ul#invert a').forEach(function(el, i) {
			el.onclick = function(e) {
				if (!el.classList.contains('current')) {
					document.querySelectorAll('ul#invert a').forEach(function(el, i) {
						el.classList.remove('current');
					});
					el.classList.add('current');
					reverse();
				}
				e.preventDefault();
			}
		});
		
		// https://stackoverflow.com/questions/36209912/alphabetize-a-list-using-js-or-jquery
		document.querySelectorAll('ul#order a').forEach(function(el, i) {
			el.onclick = function(e) {
				document.querySelectorAll('ul#order a').forEach(function(el, i) {
					el.classList.remove('current');
				});
				el.classList.add('current');
				
				['place', 'alphabet', 'relevance', 'views', 'created', 'updated', 'rate'].forEach(function(el, i) {
					document.querySelectorAll('.' + el).forEach(function(ele, ie) {
						ele.style.display = 'none';
					});
				});
				
				document.querySelectorAll('.' + el.id).forEach(function(el, i) {
					el.style.display = 'block';
				});
				
				// selector to parent element (best approach by id)
				var parent = document.querySelector("ul#feed"),
				    // take items (parent.children) into array
				    itemsArray = Array.prototype.slice.call(parent.children);
				
				if (el.id == 'relevance' || el.id == 'views') {
					// sort items in array by custom criteria
					itemsArray.sort(function (a, b) {
					    // inner text suits best (even when formated somehow)
					    //(el);
					    if (parseInt(eval('a.dataset.' + el.id)) < parseInt(eval('b.dataset.' + el.id))) return 1;
					    if (parseInt(eval('a.dataset.' + el.id)) > parseInt(eval('b.dataset.' + el.id))) return -1;
					    return 0;
					});
				} else {
					// sort items in array by custom criteria
					itemsArray.sort(function (a, b) {
					    // inner text suits best (even when formated somehow)
					    //alert(el);
					    if (eval('a.dataset.' + el.id) < eval('b.dataset.' + el.id)) return 1;
					    if (eval('a.dataset.' + el.id) > eval('b.dataset.' + el.id)) return -1;
					    return 0;
					});
				}
				
				// reorder items in the DOM
				itemsArray.forEach(function (item) {
				    // one by one move to the end in correct order
				    parent.appendChild(item);
				});
				alert
				document.querySelectorAll('ul#invert a').forEach(function(el, i) {
					el.classList.remove('current');
				});
				document.querySelector('ul#invert a:first-child').classList.add('current');
				
				e.preventDefault();
			}
		});
	}());
	//}
</script>


